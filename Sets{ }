#accessing items to another set by using update keyword
set1={"stars","moon","sun"}
set2={"galaxy","shine","milkyway"}
set1.update(set2)
print(set1)

#removing items by using remove()
set1={"stars","moon","sun"}
set1.remove("stars")
print(set1)

#removing items by using discard() method 
set1={"stars","moon","sun"}
set1.remove("stars")
print(set1)

#by using pop() method
thisset = {"apple", "banana", "cherry"}

x = thisset.pop()

print(x) #removed item

print(thisset) #the set after removal
